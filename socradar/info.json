{
  "name": "socradar",
  "version": "1.0.0",
  "label": "SOCRadar",
  "description": "SOCRadar Lookup Connector",
  "publisher": "Fortinet Turkey",
  "icon_small_name": "small.png",
  "icon_large_name": "large.png",
  "cs_approved": false,
  "cs_compatible": true,
  "category": "investigation",
  "help_online": "https://platform.socradar.com/docs/api/",
  "configuration": {
    "fields": [
      {
        "title": "URL",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "text",
        "description": "SOCRadar Threat Intelligence Feeds",
        "name": "url",
        "tooltip": "Type SOCRadar feed URL",
        "value": "https://platform.socradar.com/api"
      },
      {
        "title": "Verify SSL",
        "required": false,
        "editable": true,
        "visible": true,
        "type": "checkbox",
        "value": "true",
        "name": "verify"
      },
      {
        "title": "Retention",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "integer",
        "description": "Threat list expire limit",
        "name": "expire_hours",
        "tooltip": "Minimum 2 Hours",
        "value": "12"
      },
      {
        "title": "Company Key",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "password",
        "description": "SOCRadar Company API Key",
        "name": "company_key",
        "tooltip": "Create a key at company settings",
        "placeholder": "xxxxxxxxx"
      },
      {
        "title": "Malware Analysis Key",
        "required": true,
        "editable": true,
        "visible": true,
        "type": "password",
        "description": "SOCRadar Malware Analysis API Key",
        "name": "malware_key",
        "tooltip": "Please obtain your SOCRadar malware analysis API key by contacting with our Operation Team, operation@socradar.io",
        "placeholder": "xxxxxxxxx"
      }
    ]
  },
  "operations": [
    {
      "operation": "lookup_ioc",
      "title": "Lookup IOC",
      "description": "SOCRadar ioc lookup",
      "category": "investigation",
      "annotation": "lookup_ioc",
      "parameters": [
        {
          "title": "IOC",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "ioc",
          "description": "SOCRadar IOC ID"
        },
        {
          "title": "Search data",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "search",
          "description": "Can be ip address, hostname, hash, url, etc. depends on source"
        }
      ],
      "enabled": true,
      "output_schema": {
        "found": "",
        "extra_info": {
        },
        "feed": "",
        "feed_type": "",
        "first_seen_date": "",
        "latest_seen_date": "",
        "maintainer_name": ""
      }
    },
    {
      "operation": "malware_analysis",
      "title": "Malware Analysis",
      "description": "You can obtain the 'trust' score (out of 100) of an entity that is calculated based on various threat feed evaluations conducted by SOCRadar",
      "category": "investigation",
      "annotation": "malware_analysis",
      "parameters": [
        {
          "title": "entity",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "entity",
          "description": "Intended entity to be analyzed. Valid entity types: [IPv4, IPv6, Domain, Hash]"
        },
        {
          "title": "Advance Investigation",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "checkbox",
          "name": "advance_investigation",
          "description": "This parameter can be used to have an advanced malware analysis over the entity.",
          "value": "false"
        },
        {
          "title": "Force New Analysis",
          "required": false,
          "editable": true,
          "visible": true,
          "type": "checkbox",
          "name": "force_new_analysis",
          "description": "Malware analysis results of an entity are being cached for 24 hours after result of the entity being obtained by SOCRadar.",
          "value": "false"
        }
      ],
      "enabled": true,
      "output_schema": {
        "is_success": "",
        "message": "",
        "response_code": "",
        "data": ""
      }
    }
  ],
  "playbooks": [
    {}
  ]
}