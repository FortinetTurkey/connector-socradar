    {
      "operation": "change_status",
      "title": "Change Status",
      "description": "You can change status of an alert on the SOCRadar",
      "category": "investigation",
      "annotation": "change_status",
      "parameters": [
        {
          "title": "Alarm ID",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "integer",
          "name": "alarm_id",
          "description": "Alarm ID"
        },
        {
          "title": "Status",
          "name": "status",
          "required": "true",
          "editable": "true",
          "visible": "true",
          "type": "select",
          "options": [
            "OPEN",
            "INVESTIGATING",
            "PENDING_INFO",
            "LEGAL_REVIEW",
            "VENDOR_ASSESSMENT",
            "FALSE_POSITIVE",
            "DUPLICATE",
            "PROCESSED_INTERNALLY",
            "NOT_APPLICABLE",
            "MITIGATED",
            "RESOLVED"
          ],
          "description": "Status of the alerts"
        },
        {
          "title": "Comments",
          "required": true,
          "editable": true,
          "visible": true,
          "type": "text",
          "name": "comments",
          "description": "Comments about the incident will be shown under the Resolve Notes section in the incident details."
        }
      ],
